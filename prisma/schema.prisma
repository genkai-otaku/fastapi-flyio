generator client {
  provider             = "prisma-client-py"
  interface            = "asyncio"
  recursive_type_depth = 5
}

datasource db {
  provider = "postgresql"
}

model Memo {
  id         String   @id @default(cuid())
  title      String
  content    String
  created_at DateTime @default(now())
  updated_at DateTime @updatedAt
}
